{
  "expo": {
    "name": "AarambhApp",
    "slug": "AarambhApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/R.png",
    "scheme": "aarambhapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AarambhApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/R.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.AarambhApp",
      "usesCleartextTraffic": true,
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ],
      "allowBackup": false,
      "hardwareAccelerated": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/R.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/R.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1b9deec-713a-49df-8eb4-ab35b844d7b4"
      },
      "razorpayKeyId": "rzp_test_R5asFuaFqiFCMM",
      "apiBaseUrl": "https://aarambh-english-learning-app-1.onrender.com/api"
    },
    "owner": "akashhsr"
  }
}
